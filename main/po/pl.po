"{0} (expression) { \n"
"  {1} constant-expression:\n"
"    statement\n"
"    jump-statement\n"
"  [{2}:\n"
"    statement\n"
"    jump-statement]\n"
"}"
msgstr ""
"{0} (wyrażenie) { \n"
"  {1} wyrażenie_stałe:\n"
"    instrukcja\n"
"    instrukcja_skoku\n"
"  [{2}:\n"
"    instrukcja\n"
"    instrukcja_skoku]\n"
"}"

#: ../src/addins/CSharpBinding/MonoDevelop.CSharp/SignatureMarkupCreator.cs:1097
msgid ""
"{0} (Type)\n"
"\n"
"or\n"
"\n"
"{1} (expression) { \n"
