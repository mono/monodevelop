{
	"name": "swift",
	"version": "0.1.0",
	"publisher": "vscode",
	"engines": { "vscode": "*" },
	"contributes": {
		"languages": [{
			"id": "swift",
			"aliases": ["Swift","swift"],
			"extensions": [".swift"],
			"configuration": "./language-configuration.json"
		}],
		"grammars": [{
			"language": "swift",
			"scopeName": "source.swift",
			"path": "./syntaxes/swift.json"
		}],
		"snippets": [{
			"language": "swift",
			"path": "./snippets/swift.json"
		}]
	}
}