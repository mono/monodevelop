  <Choose>
    <When Condition=" Exists('c:\Program Files\Git\bin\git.exe') ">
      <PropertyGroup>
        <Git>c:\Program Files\Git\bin\git.exe</Git>
      </PropertyGroup>
    </When>
    <When Condition=" Exists('c:\Program Files (x86)\Git\bin\git.exe') ">
