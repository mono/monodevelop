<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\..\..\MonoDevelop.props" />
  <Import Project="$(ReferencesGtk)" />
  <PropertyGroup>
    <Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>
    <Platform Condition=" '$(Platform)' == '' ">AnyCPU</Platform>
<TargetFramework>$(MDTargetFrameworkNet5)</TargetFramework>
    <OutputPath>..\..\..\..\build\AddIns\MonoDevelop.Debugger.Soft</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' " />
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' " />
  <ItemGroup>
    <Reference Include="System" />
    <Reference Include="System.Core" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="AssemblyInfo.cs" />
    <Compile Include="SoftDebuggerEngine.cs" />
    <Compile Include="CustomSoftDebuggerEngine.cs" />
    <Compile Include="AddinInfo.cs" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Manifest.addin.xml" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\MonoDevelop.Debugger\MonoDevelop.Debugger.csproj">
      <Private>False</Private>
    </ProjectReference>
    <ProjectReference Include="..\..\..\core\MonoDevelop.Ide\MonoDevelop.Ide.csproj">
      <Private>False</Private>
    </ProjectReference>
    <ProjectReference Include="..\..\..\core\MonoDevelop.Core\MonoDevelop.Core.csproj">
      <Private>False</Private>
    </ProjectReference>
    <ProjectReference Include="..\..\..\..\external\debugger-libs\Mono.Debugging\Mono.Debugging.csproj">
      <Private>False</Private>
    </ProjectReference>
    <ProjectReference Include="..\..\..\..\external\debugger-libs\Mono.Debugging.Soft\Mono.Debugging.Soft.csproj">
    </ProjectReference>
    <ProjectReference Include="..\..\..\..\external\debugger-libs\Mono.Debugger.Soft\Mono.Debugger.Soft.csproj">
    </ProjectReference>
    <ProjectReference Include="..\..\..\..\external\mono-addins\Mono.Addins\Mono.Addins.csproj">
      <Private>False</Private>
    </ProjectReference>
    <IncludeCopyLocal Include="Mono.Debugger.Soft.dll" />
    <IncludeCopyLocal Include="Mono.Debugging.Soft.dll" />
  </ItemGroup>

</Project>
