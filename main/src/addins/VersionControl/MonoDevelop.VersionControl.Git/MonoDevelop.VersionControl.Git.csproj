    </None>
    <None Include="..\..\..\..\external\libgit-binary\libgit2.license.txt">
      <Link>libgit2.license.txt</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="..\..\..\..\external\libgit-binary\libssh2.license.txt">
