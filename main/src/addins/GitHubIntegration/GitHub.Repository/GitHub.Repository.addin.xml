<ExtensionModel>
	<Runtime>
		<Import assembly="Octokit.dll" />
	</Runtime>

	<Extension path = "/MonoDevelop/Ide/Pads">

		<Pad id = "GitHub.Repository.Gui.GitHubPad"
			class = "GitHub.Repository.Gui.GitHubPad"
			_label="My GitHub"
			icon = "nunit-run"
			defaultLayout="*"
			defaultPlacement="Rright"
			defaultStatus="AutoHide"
			/>
	</Extension>

	<Extension path = "/MonoDevelop/Core/StockIcons">
		<StockIcon stockid = "nunit-run" resource = "pad-unit-test-light-16.png" size="Menu" />
	</Extension>
</ExtensionModel>
