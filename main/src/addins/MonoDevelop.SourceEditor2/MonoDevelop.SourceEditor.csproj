    <StartProgram>mono</StartProgram>
    <StartArguments>--debug ../../../build/bin/MonoDevelop.exe</StartArguments>
    <ExternalConsole>true</ExternalConsole>
    <ConsolePause>false</ConsolePause>
  </PropertyGroup>
