<Addin id = "Mono.Debugging.Gdb"
       name	     = "GDB support for Mono.Debugging"
       author	 = "Lluis Sanchez"
       url       = "http://www.mono-project.com"
       description = "GNU Debugger support for Mono.Debugging"
       copyright   = "MIT X11"
       version   = "0.1">

	<Dependencies>
		<Addin id="MonoDevelop.Core" version="1.9.1"/>
		<Addin id="MonoDevelop.Ide" version="1.9.1"/>
		<Addin id="MonoDevelop.Debugger" version="1.9.1"/>
	</Dependencies>
	
	<Extension path="/MonoDevelop/Debugging/DebuggerFactories">
		<DebuggerFactory type="MonoDevelop.Debugger.Gdb.GdbSessionFactory" />
	</Extension>

</Addin>
